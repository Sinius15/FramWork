<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
           version="3.0">

    <servlet>
        <servlet-name>Jaravel</servlet-name>
        <servlet-class>jaravel.deployment.servlet.Servlet</servlet-class>
        <init-param>
            <param-name>FrameworkClass</param-name>
            <param-value>jaravel.example.TestFramework</param-value>
        </init-param>
    </servlet>
    <servlet-mapping>
        <servlet-name>Jaravel</servlet-name>
        <url-pattern>/*</url-pattern>
    </servlet-mapping>

</web-app>
